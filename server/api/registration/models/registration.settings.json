{
  "kind": "collectionType",
  "collectionName": "registrations",
  "info": {
    "name": "Registration"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "FirstName": {
      "type": "string"
    },
    "LastName": {
      "type": "string"
    },
    "Password": {
      "type": "password"
    },
    "Email": {
      "type": "email"
    },
    "Phone": {
      "type": "biginteger"
    },
    "PostalCode": {
      "type": "biginteger"
    },
    "Address": {
      "type": "text"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    }
  }
}
